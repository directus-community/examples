{
  "name": "directus-remix-example",
  "private": true,
  "scripts": {
    "build": "remix build",
    "dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "dev:windows": "node -r dotenv/config ./node_modules/@remix-run/dev/cli.js dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "seed": "node ../shared/seed --project=Remix"
  },
  "dependencies": {
    "@directus/sdk": "9.4.3",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "dotenv": "14.2.0",
    "modern-normalize": "1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
